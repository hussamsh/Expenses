<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.infinitetech.expenses.ActivityClasses.IncomeFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Latest Income"
            android:gravity="center"
            android:layout_marginTop="90dp"
            android:textStyle="bold|italic"
            android:textColor="@color/actionbar_color"
            android:textSize="21sp"
            android:layout_marginBottom="30dp"/>

        <TextView
            android:id="@+id/latest_income_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:gravity="center"
            android:textStyle="bold|italic"
            android:textSize="21sp"
            android:layout_marginBottom="30dp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Date"
            android:gravity="center"
            android:textStyle="bold|italic"
            android:textColor="@color/actionbar_color"
            android:textSize="21sp"
            android:layout_marginBottom="30dp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/income_date_text"
            android:textSize="21sp"
            android:layout_marginBottom="30dp"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="21sp"
            android:text="Method Of Reciving"
            android:gravity="center"
            android:textColor="@color/actionbar_color"
            android:textStyle="bold|italic"
            android:layout_marginBottom="30dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/income_methodOfReceiving_text"
            android:gravity="center"
            android:textSize="21sp" />
    </LinearLayout>

</FrameLayout>
